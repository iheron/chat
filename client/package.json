{
  "name": "chat_client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ENV=localhost babel-node ./server",
    "dev-client": "next",
    "dev-server": "cross-env NODE_ENV=development nodemon --exec node -r @babel/register ./server",
    "build": "cross-env NODE_ENV=production ENV=production next build && webpack --bail -p --config ./webpack/webpack.prod.js",
    "build-dev": "cross-env NODE_ENV=production ENV=development next build && webpack --bail -p --config ./webpack/webpack.prod.js",
    "build-client": "cross-env NODE_ENV=production ENV=development next build",
    "build-server": "cross-env NODE_ENV=production ENV=development webpack --bail -p --config ./webpack/webpack.prod.js",
    "start": "node ./dist",
    "start-client": "next start"
  },
  "dependencies": {
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.18.1",
    "antd-mobile": "^2.2.13",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.2",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^0.1.19",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "js-cookie": "^2.2.0",
    "less": "^3.0.4",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.6.0",
    "morgan": "^1.9.0",
    "next": "^8.1.0",
    "nkn-client": "^0.5.0",
    "nkn-wallet": "^0.2.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "react": "^16.8.6",
    "react-cookie": "^2.2.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {}
}
